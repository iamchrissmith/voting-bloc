= form_for [:admin, @election] do |f|
  = render partial: "shared/errors", locals: {target: @election}
  .form-group
    = f.label :start_date
    = f.date_field :start_date
  .form-group
    = f.label :end_date
    = f.date_field :end_date
  .form-group
    = f.label :topic
    = f.text_field :topic
  .form-group
    = f.label :description
    = f.text_area :description
  .form-group
    %p Select Candidates to Participate:
    = f.collection_check_boxes(:candidate_ids, @candidates, :id, :full_name)
  = f.submit
